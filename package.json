    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "coveralls": "^2.12.0",
    "cross-env": "^3.1.4",
    "eslint": "^3.18.0",
